<h1> <%= @current_user.user_name %>'s Profile</h1>

<% if @user.id == @current_user.id %>
<%= link_to "signout", log_out_path %>
<% end %>

<% if @user.id == @current_user.id %>
<%= link_to("Delete", user_path(@user), method: :delete) %>
<% end %>

<%= tag :br %>

<% @user.jobs.each do |j| %>
<%= j.text_origin %>
...
<%= j.job_name %>
<%= tag :br %>
<% end %>
<%= tag :br %>
<% @user.boats.each do |b| %>
<%= b.name %>
...
<%= b.location %>
<%= tag :br %>
<% end %>

